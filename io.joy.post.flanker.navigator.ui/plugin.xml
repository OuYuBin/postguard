<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="com.gesila.test.guard.ui.category"
            class="com.gesila.test.guard.navigator.ui.views.GesilaTestGuardCommonNavigator"
            icon="icons/testguardexplorer.png"
            id="com.gesila.test.guard.navigator.ui.views.GesilaTestGuardCommonNavigator"
            name="PostFlanker导航"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewer
            viewerId="com.gesila.test.guard.navigator.ui.views.GesilaTestGuardCommonNavigator">
         <popupMenu
               id="com.gesila.test.guard.navigator.ui.views.GesilaTestGuardCommonNavigator#PopupMenu">
            <insertionPoint
                  name="group.new"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.edit"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.import">
            </insertionPoint>
         </popupMenu>
      </viewer>
      <viewerActionBinding
            viewerId="com.gesila.test.guard.navigator.ui.views.GesilaTestGuardCommonNavigator">
         <includes>
            <contentExtension
                  pattern="com.gesila.test.guard.navigator.ui.views.providers.GesilaTestGuardCommonActionProvider">
            </contentExtension>
            <actionExtension
                  pattern="com.gesila.test.guard.navigator.ui.views.providers.GesilaTestGuardOpenActionProvider">
            </actionExtension>
         </includes>
      </viewerActionBinding>
      <viewerContentBinding
            viewerId="com.gesila.test.guard.navigator.ui.views.GesilaTestGuardCommonNavigator">
         <includes>
            <contentExtension
                  pattern="com.gesila.test.guard.navigator.ui.navigatorContent">
            </contentExtension>
            <contentExtension
                  pattern="com.gesila.test.guard.navigator.ui.views.linkHelper.GesialLinkTestGuardHelper">
            </contentExtension>
         </includes>
      </viewerContentBinding>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            contentProvider="com.gesila.test.guard.navigator.ui.views.providers.GesilaTestGuardTreeContentProvider"
            icon="icons/project.png"
            id="com.gesila.test.guard.navigator.ui.navigatorContent"
            labelProvider="com.gesila.test.guard.navigator.ui.views.providers.PostFlankerTreeLabelProvider"
            name="PostGuard Navigator Content"
            priority="higher">
         <triggerPoints>
            <instanceof
                  value="org.eclipse.core.resources.IWorkspaceRoot">
            </instanceof>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IResource">
               </instanceof>
               <instanceof
                     value="com.gesila.test.guard.project.models.IGesilaTestGuardProjectElement">
               </instanceof>
            </or>
         </possibleChildren>
         <actionProvider
               class="com.gesila.test.guard.navigator.ui.views.providers.GesilaTestGuardCommonActionProvider"
               id="com.gesila.test.guard.navigator.ui.views.providers.GesilaTestGuardCommonActionProvider">
            <enablement>
               <or>
                  <adapt
                        type="java.util.Collection">
                     <count
                           value="0">
                     </count>
                  </adapt>
                  <instanceof
                        value="io.joy.post.flanker.project.models.IPostFlankerProject">
                  </instanceof>
                  <instanceof
                        value="io.joy.post.flanker.project.models.impl.PostFlankerGroup">
                  </instanceof>
                  <instanceof
                        value="io.joy.post.flanker.project.models.impl.PostFlankerDependence">
                  </instanceof>
               </or>
            </enablement>
         </actionProvider>
         <override
               policy="InvokeAlwaysRegardlessOfSuppressedExt"
               suppressedExtensionId="org.eclipse.ui.navigator.resourceContent">
         </override>
         <commonWizard
               type="new"
               wizardId="com.gesila.test.guard.navigator.ui.wizards.GesilaTestGuardNewProjectWizard">
            <enablement></enablement>
         </commonWizard>
         <actionProvider
               class="com.gesila.test.guard.navigator.ui.views.providers.GesilaTestGuardOpenActionProvider"
               id="com.gesila.test.guard.navigator.ui.views.providers.GesilaTestGuardOpenActionProvider">
            <enablement>
               <not>
                  <instanceof
                        value="io.joy.post.flanker.project.models.IPostFlankerProjectContainerElement">
                  </instanceof>
               </not>
               <or>
                  <instanceof
                        value="io.joy.post.flanker.project.models.impl.PostFlankerTestGuard">
                  </instanceof>
                  <instanceof
                        value="io.joy.post.flanker.project.models.impl.PostFlankerLibrary">
                  </instanceof>
               </or>
            </enablement>
         </actionProvider>
         <actionProvider
               class="com.gesila.test.guard.navigator.ui.views.providers.PostGuardEditCommonActionProvider"
               id="com.gesila.test.guard.navigator.ui.views.providers.PostGuardEditCommonActionProvider">
            <enablement>
               <or>
                  <instanceof
                        value="io.joy.post.flanker.project.models.impl.PostFlankerLibrary">
                  </instanceof>
                  <instanceof
                        value="io.joy.post.flanker.project.models.impl.PostFlankerTestGuard">
                  </instanceof>
                  <instanceof
                        value="io.joy.post.flanker.project.models.IPostFlankerProjectContainerElement">
                  </instanceof>
               </or>
            </enablement>
         </actionProvider>
         <commonSorter
               class="com.gesila.test.guard.navigator.ui.views.comparator.PostGuardViewerComparator">
         </commonSorter>
      </navigatorContent>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="com.gesila.test.guard.navigator.ui.category"
            class="com.gesila.test.guard.navigator.ui.wizards.GesilaTestGuardNewProjectWizard"
            finalPerspective="com.gesila.test.guard.application.e3.perspective"
            icon="icons/project.png"
            id="com.gesila.test.guard.navigator.ui.wizards.GesilaTestGuardNewProjectWizard"
            name=" PostFlanker项目"
            project="true">
      </wizard>
      <category
            id="com.gesila.test.guard.navigator.ui.category"
            name="PostFlanker">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:test.guard.new.menu?after=additions">
         <command
               commandId="com.gesila.test.guard.navigator.ui.views.handlers.GesilaTestGuardNewGroupHandler"
               icon="icons/group.gif"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="com.gesila.test.guard.navigator.ui.views.handlers.GesilaTestGuardNewRequestHandler"
               icon="icons/interface.png"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="com.gesila.test.guard.navigator.ui.views.handlers.PostGuardNewLibHandler"
               icon="icons/library.png"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.gesila.test.guard.navigator.ui.views.handlers.GesilaTestGuardNewGroupHandler"
            name="模块">
      </command>
      <command
            id="com.gesila.test.guard.navigator.ui.views.handlers.PostGuardNewLibHandler"
            name="依赖">
      </command>
      <command
            id="com.gesila.test.guard.navigator.ui.views.handlers.GesilaTestGuardNewRequestHandler"
            name="接口">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.gesila.test.guard.navigator.ui.views.handlers.GesilaTestGuardNewGroupHandler"
            commandId="com.gesila.test.guard.navigator.ui.views.handlers.GesilaTestGuardNewGroupHandler">
         <enabledWhen>
            <and>
               <not>
                  <adapt
                        type="java.util.Collection">
                     <count
                           value="0">
                     </count>
                  </adapt>
               </not>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="true">
                     <instanceof
                           value="io.joy.post.flanker.project.models.IPostFlankerProject">
                     </instanceof>
                  </iterate>
               </with>
            </and>
         </enabledWhen>
      </handler>
      <handler
            class="com.gesila.test.guard.navigator.ui.views.handlers.GesilaTestGuardNewRequestHandler"
            commandId="com.gesila.test.guard.navigator.ui.views.handlers.GesilaTestGuardNewRequestHandler">
         <enabledWhen>
            <and>
               <not>
                  <adapt
                        type="java.util.Collection">
                     <count
                           value="0">
                     </count>
                  </adapt>
               </not>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="true">
                     <instanceof
                           value="io.joy.post.flanker.project.models.impl.PostFlankerGroup">
                     </instanceof>
                  </iterate>
               </with>
            </and>
         </enabledWhen>
      </handler>
      <handler
            class="com.gesila.test.guard.navigator.ui.views.handlers.PostGuardNewLibHandler"
            commandId="com.gesila.test.guard.navigator.ui.views.handlers.PostGuardNewLibHandler">
         <enabledWhen>
            <and>
               <not>
                  <adapt
                        type="java.util.Collection">
                     <count
                           value="0">
                     </count>
                  </adapt>
               </not>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="true">
                     <instanceof
                           value="io.joy.post.flanker.project.models.impl.PostFlankerDependence">
                     </instanceof>
                  </iterate>
               </with>
            </and>
         </enabledWhen>
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.linkHelper">
      <linkHelper
            class="com.gesila.test.guard.navigator.ui.views.linkHelper.GesialLinkTestGuardHelper"
            id="com.gesila.test.guard.navigator.ui.views.linkHelper.GesialLinkTestGuardHelper">
         <editorInputEnablement>
            <instanceof
                  value="org.eclipse.ui.IEditorInput">
            </instanceof></editorInputEnablement>
         <selectionEnablement>
            <instanceof
                  value="com.gesila.test.guard.project.models.IGesilaTestGuardProjectElement">
            </instanceof></selectionEnablement>
      </linkHelper>
   </extension>

</plugin>
